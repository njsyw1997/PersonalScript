{
    "problem": "GenericTensor",
    "tensor_formulation": "NeoHookean",
    "discr_order": 1,
    "n_refs": 0,
    "problem_params": {
        "is_time_dependent": true,
        "rhs": [0, 0, 0],
        "dirichlet_boundary": [{
            "id": 2,
            "value": [
                "x*cos(-2*pi*t) + z * sin(-2*pi*t)-x",
                "-1.33*0.01*t", 
                "-1*x*sin(-2*pi*t) + z * cos(-2*pi*t)-z"
            ],
            "dimension": [true, false, true]
        }]
    },
    "nl_solver": "newton",
    "solver_params": {
		"Pardiso": {
			"mtype": 2,
            "gradNorm": 1e-5,
            "useGradNorm": false
		},
		"Hypre": {
			"max_iter": 1000,
			"pre_max_iter": 1000,
			"tolerance": 1e-10,
            "gradNorm": 1e-5,
            "useGradNorm": false
		},
		"AMGCL": {
			"precond": {
				"relax": {
					"degree": 5,
					"type": "chebyshev",
					"power_iters": 100,
					"higher": 2,
					"lower": 0.008333333333,
					"scale": true
				},
				"class": "amg",
				"max_levels": 6,
				"direct_coarse": false,
				"ncycle": 1,
				"coarsening": {
					"type": "smoothed_aggregation",
					"estimate_spectral_radius": true,
					"relax": 1,
					"aggr": {
						"eps_strong": 0
					}
				}
			},
			"solver": {
				"tol": 1e-8,
				"maxiter": 1000,
				"type": "cg"
			},
            "gradNorm": 1e-5,
            "useGradNorm": false
		}
    },
    "solver_type": "Eigen::PardisoLDLT",
    "line_search": "backtracking",
    "project_to_psd": true,
    "lump_mass_matrix": true,

    "has_collision": true,
    "dhat": 1e-4,

    "normalize_mesh": false,

    "export": {
        "paraview": "sol.vtu",
        "body_ids": true,
        "material_params": true,
        "time_sequence": "screw.pvd"
    },
    "quadrature_order": 1,
    "save_time_sequence": true,
    "save_solve_sequence_debug": false,
    "vismesh_rel_area": 10000000,
    "output": "sim.json",
    "tend": 2,
    "dt": 0.01,
    
    "meshes": [{
        "mesh": "../../mesh/screw/screw.msh",
        "scale": 0.01,
        "body_id": 1
    }],
    "body_params": [{
        "id": 1,
        "E": 200e9,
        "nu": 0.28,
        "rho": 8050
    }],
    "obstacles": [{
        "mesh": "/home/yiwei/PersonalScript/polyfem/mesh/screw/nut.obj",
        "scale": 0.01
    }]
}