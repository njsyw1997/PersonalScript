{
    "problem": "GenericTensor",
    "tensor_formulation": "NeoHookean",
    "nl_solver": "newton",
    "solver_params": {
		"Pardiso": {
			"mtype": 2,
            "gradNorm": 1e-5,
            "useGradNorm": false
		},
		"Hypre": {
			"max_iter": 1000,
			"pre_max_iter": 1000,
			"tolerance": 1e-10,
            "gradNorm": 1e-5,
            "useGradNorm": false
		},
		"AMGCL": {
			"precond": {
				"relax": {
					"degree": 16,
					"type": "chebyshev",
					"power_iters": 100,
					"higher": 2,
					"lower": 0.008333333333,
					"scale": true
				},
				"class": "amg",
				"max_levels": 6,
				"direct_coarse": false,
				"ncycle": 2,
				"coarsening": {
					"type": "smoothed_aggregation",
					"estimate_spectral_radius": true,
					"relax": 1,
					"aggr": {
						"eps_strong": 0
					}
				}
			},
			"solver": {
				"tol": 1e-10,
				"maxiter": 1000,
				"type": "cg"
			},
            "gradNorm": 1e-5,
            "useGradNorm": false
		}
    },
    "solver_type": "Eigen::PardisoLDLT",
    "line_search": "backtracking",
    "project_to_psd": true,
    "lump_mass_matrix": true,

    "has_collision": true,
    "dhat": 1e-3,

    "normalize_mesh": false,

    "export": {
        "paraview": "sol.vtu",
        "body_ids": true,
        "material_params": true
    },
    "quadrature_order": 1,
    "save_time_sequence": true,
    "save_solve_sequence_debug": false,
    "vismesh_rel_area": 10000000,
    "output": "sim.json",    
    "problem_params": {
        "is_time_dependent": true,
        "dirichlet_boundary": [{
            "id": 1,
            "value": [0, "y*cos((-0.4*pi)*t)-z*sin((-0.4*pi)*t)-y", "y*sin((-0.4*pi)*t)+z*cos((-0.4*pi)*t)-z"]
        }, {
            "id": 3,
            "value": [0, "y*cos((0.4*pi)*t)-z*sin((0.4*pi)*t)-y", "y*sin((0.4*pi)*t)+z*cos((0.4*pi)*t)-z"]
        }],
        "rhs": [0, 0, 0]
    },
    "tend": 4,
    "dt": 0.04,
    "meshes": [{
        "mesh": "../../../meshes/3D/simple/mat/mat40x40.msh",
        "position": [0, 0, 0],
        "rotation": [90, 0, 0],
        "body_id": 1
    }],
    "body_params": [{
        "id": 1,
        "E": 20000,
        "nu": 0.4,
        "rho": 1000
    }],
    "boundary_sidesets": [{
        "id": 1,
        "axis": 1,
        "position": 0.5
    }, {
        "id": 3,
        "axis": -1,
        "position": -0.5
    }],
    "boundary_id_threshold": 1e-5
}